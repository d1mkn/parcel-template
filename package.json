{
  "name": "parcel-template",
  "version": "2.1.0",
  "description": "Modern frontend starter template using Parcel 2",
  "homepage": "https://github.com/d1mkn/parcel-template",
  "scripts": {
    "start": "parcel src/*.html --port 2222",
    "build": "parcel build src/*.html --public-url ./",
    "clean": "rm -rf dist .parcel-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d1mkn/parcel-template"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.13.0",
    "parcel": "^2.13.0",
    "parcel-reporter-clean-dist": "^1.0.4",
    "posthtml-include": "^1.7.4",
    "sass": "^1.77.0"
  },
  "dependencies": {
    "modern-normalize": "^2.0.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead"
}
